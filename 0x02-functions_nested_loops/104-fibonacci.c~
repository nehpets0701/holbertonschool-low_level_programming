#include <stdio.h>

/**
 *main-main
 *Return:0
 */
int main(void)
{
	unsigned long int i;
	unsigned long int a = 1;
	unsigned long int b = 2;
	unsigned long int c;

	for (i = 0; i < 98; i++)
	{
		if (i < 97)
		{
			printf("%li, ", a);
			c = b + a;
			a = b;
			b = c;
		}
		else
			printf("%li\n", a);
	}
	return (0);
}
