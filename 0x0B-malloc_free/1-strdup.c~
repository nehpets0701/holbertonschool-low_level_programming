#include "holberton.h"

/**
 *_strdup-strdup
 *Return:char
 *@str:input
 */
char *_strdup(char *str)
{
	int i = 0;
	int j;
	char str2[];
	char *ptr;

	while (str[i] != 0)
		i ++;
	ptr = malloc((sizeof(char) * i) + 1);
	if (ptr == NULL)
		return (NULL);

	for (j = 0 ; str[j] != '\0' ; j++)
	{
		str2[j] = str[j];
	}
}
